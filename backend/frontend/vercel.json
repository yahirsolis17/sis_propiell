{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*\\.js)",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "src": "/assets/(.*\\.woff2)",
      "headers": { "Content-Type": "font/woff2" }
    },
    {
      "src": "/assets/(.*\\.woff)",
      "headers": { "Content-Type": "font/woff" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": { "Content-Type": "text/html" }
    }
  ]
}