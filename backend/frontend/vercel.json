{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)\\.woff2",
      "headers": {
        "Content-Type": "font/woff2"
      }
    },
    {
      "src": "/assets/(.*)\\.woff",
      "headers": {
        "Content-Type": "font/woff"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}